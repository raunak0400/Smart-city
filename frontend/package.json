    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
